/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    Not (in=a, out=NOTa);
    Not (in=b, out=NOTb);
    And (a=NOTa, b=NOTb, out=AND1);
    And (a=a, b=b, out=AND2);
    Or (a=AND1, b=AND2, out=out1);
    Not (in=out1, out=out);
}